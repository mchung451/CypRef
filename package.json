  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npx cypress run",
    "headTest": "npm run test -- --headed",
    "chromeTest": "npm run test -- --browser chrome --parallel",
    "recordDashBoardTest": "npm run test -- --record --key d688b87f-a4c2-458a-99be-ce94e397b3f7 --reporter mochawesome",
    "GreenKartTest": "npm run test -- --spec \"cypress//integration//GreenKart/*\"",
    "cucufeaturechrome": "cypress run --spec cypress/integration/*.feature --browser chrome"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
