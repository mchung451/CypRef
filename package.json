  "license": "ISC",
  "devDependencies": {
    "cypress": "^9.2.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-iframe": "^1.0.1",
    "mocha": "^9.1.3",
    "mochawesome": "^7.0.1",
    "multiple-cucumber-html-reporter": "^1.18.3"
  }
